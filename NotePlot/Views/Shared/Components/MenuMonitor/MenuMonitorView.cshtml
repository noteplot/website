            <ul class="navigation">
                @*<li><a href="#">Параметры измерения</a></li>*@
                <li controller-name="Analytics" action-name="ReportMonitor">@Html.ActionLink("Измерения по монитору", "ReportMonitor", "Analytics")</li>
                <li controller-name="Analytics" action-name="ReportPlot">@Html.ActionLink("Динамика изменений параметров", "ReportPlot", "Analytics")</li>
            </ul>
        </li>
    </ul>
