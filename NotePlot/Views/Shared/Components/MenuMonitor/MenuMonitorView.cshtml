@model bool

<div id="menuMonitor">
@if (Model) {
    <ul class="navigation">
        <li>
            <a class="button no-href">
                <div class="icon"><span class="fa fa-sliders-h"></span></div>
                <div class="name">Измерения</div>
            </a>
            <ul class="navigation">
                <li controller-name="Monitor, Monitoring" action-name="Monitor*">@Html.ActionLink("Мониторы измерений", "MonitorList", "Monitor")</li>
            </ul>
        </li>
        <li>
            <a href="#" class="button no-href">
                <div class="icon"><span class="fa fa-cog"></span></div>
                <div class="name">Справочники</div>
            </a>
            <ul class="navigation">
                <li controller-name="Parameter" action-name="Packet*">@Html.ActionLink("Пакеты параметров", "PacketList", "Parameter")</li>
                <li controller-name="Parameter" action-name="Parameter*">@Html.ActionLink("Параметры измерений", "ParameterList", "Parameter")</li>
